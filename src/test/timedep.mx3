/*
	Test time dependent parameters.
*/

c:=4e-9
setgridsize(32, 32, 1)
setcellsize(c, c, c)

Msat=860e3
Aex=13e-12
alpha=0.2
m=uniform(1, 1, 0)
anisU = vector(0, 1, 0)

f := 1e9
Ku1 = 1e5 * sin(2 * pi * f * t)
tableadd(Ku1)

run(0.5e-9)

TOL := 1e-5
expectv("m", m.average(), vector(0, 0.9909376502037048, 0), TOL)
