

// geometry
	setgridsize(128, 32, 1)
	setcellsize(500e-9/128, 125e-9/32, 3e-9)

// material
	Aex   = 13e-12
	Msat  = 800e3
	alpha = 2
	m  = uniform(1, .1, 0)

	setSolver(-1)
	FixDt = 1e-13
	run(10e-9)
	TOL := 1e-2
	expectv("m", m.average(), vector(0.9669684171676636,  0.1252732127904892, 0), TOL)

