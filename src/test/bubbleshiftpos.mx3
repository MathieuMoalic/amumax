SetMesh(128, 128, 1, 1e-9,1e-9,0.4e-9, 1, 1, 0)

Msat  =580e3
Aex   = 15e-12
enabledemag=false
alpha = 0.1
Ku1=0.59e6
anisU=vector(0,0,1)
Dind=0.0034089785

shiftregions=true

maxregion:=255
seed:=17
ext_makegrains(10e-9, maxregion, seed)

for i:=0; i<maxRegion; i++{
        for j:=i+1; j<maxRegion; j++{
                ext_ScaleExchange(i, j, 0.5)
        }
