/*
	Check if setting gammaLL has effect
*/

N := 16
c := 5e-9
setgridsize(N, N, 1)
setcellsize(c, c, c)
setSolver(2)
maxerr = 1e-4

Msat = 1/mu0
Aex  = 10e-12
alpha = 0.001

m = uniform(1, 1, 1)

B_ext = vector(0, 0, 1)

tableautosave(1e-12)

run(0.1e-9)

gammaLL = gammaLL * 2 // make sure changing at a late point still has effect
run(0.1e-9)

TOL := 1e-5
expectv("m", m.average(), vector(-0.286423, -0.746625, 0.5970549), TOL)
