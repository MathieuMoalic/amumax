setgridsize(512,512,1)
setcellsize(1e-9,1e-9,1e-9)
setpbc(0,0,0)
chunky = true
edgesmooth=3
msat = 956e3
aex = 10e-12
alpha = 0.03
setgeom(circle(500e-9))
m = vortex(1,1)
minimize()
setsolver(5)
B_ext = vector(0, 1e-2*sin(2*pi*12e9*t), 0)
tableadd(m)
tableautosave(1e-11)
autosave(m,1e-11)
run(1e-10)